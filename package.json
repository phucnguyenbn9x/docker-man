{
  "name": "docker-man",
  "version": "1.0.0",
  "description": "Docker Manager",
  "main": "main.js",
  "scripts": {
    "start": "node server/app.js",
    "window": "electron .",
    "package-app": "electron-packager . DockerMan --overwrite --asar --platform=linux --arch=x64 --prune=true --out=native-app",
    "create-installer": "electron-installer-debian --src native-app/DockerMan-linux-x64/ --arch amd64 --config debian.json",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "dev": "webpack -w --mode=development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "asar": "^0.14.6",
    "css-loader": "^2.1.0",
    "electron": "^3.1.2",
    "electron-installer-debian": "^1.1.0",
    "electron-packager": "^13.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
